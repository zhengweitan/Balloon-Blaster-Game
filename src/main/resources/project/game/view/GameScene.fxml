<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.game.view.GameSceneController">
   <children>
      <ImageView fitHeight="486.0" fitWidth="618.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Images/Cloud.gif" />
         </image>
      </ImageView>
      <Text fx:id="timerLabel" layoutX="153.0" layoutY="49.0" style="-fx-font-size: 24px; -fx-fill: black;" text="Timer: 30" textAlignment="CENTER">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Text>
      <Text fx:id="scoreLabel" layoutX="357.0" layoutY="49.0" style="-fx-font-size: 24px; -fx-fill: black;" text="Score: 0" textAlignment="CENTER">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Text>
      <AnchorPane fx:id="balloonPane" layoutX="62.0" layoutY="89.0" prefHeight="343.0" prefWidth="513.0" />
      <Button layoutX="559.0" layoutY="19.0" mnemonicParsing="false" onAction="#handlePauseButton" text="| |" />
      <Button layoutX="14.0" layoutY="19.0" mnemonicParsing="false" onAction="#handleSuperpowerButton">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="32.0">
               <image>
                  <Image url="@../../../Images/Superpower.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
